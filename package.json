    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.4.0",
    "react-sticky-box": "^1.0.2",
    "react-table": "^7.8.0",
